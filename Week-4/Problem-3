#include <bits/stdc++.h>
using namespace std;


 // } Driver Code Ends
//Complete the below functions
int maximumElement(int arr[],int n)
{
    //Your code here
int max=0;
for(int i=1;i<n;i++){
    if(arr[i]>=arr[max]){
max=i;
}
}
return arr[max];
}

int minimumElement(int arr[],int n)
{
    //Your code here
int min=0;
for(int i=1;i<n;i++){
    if(arr[i]<=arr[min])
    {
        min=i;
    }
}
return arr[min];
}

// { Driver Code Starts.


int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int arr[n];
	    for(int i=0;i<n;i++)
	    cin>>arr[i];
	    
	    cout<<maximumElement(arr,n)<<" "<<minimumElement(arr,n)<<endl;
	    
	}
	return 0;
}  
