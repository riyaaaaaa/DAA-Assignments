#include<iostream>
using namespace std;
int main()
{
    int i,n,left,right,size,mid,key;
   // cin>>n;
  //  cout<<"no.of test cases"<<n<<endl;
    cin>>size;
    int arr[size];
    for(i=0;i<size;i++)
        cin>>arr[i];
    cin>>key;
    left=0;
    right=size-1;
    mid=left+(right-left)/2;
    while(left<=right){
        if(arr[mid]<key)
        left=mid+1;
    else if(arr[mid]==key){
        cout<<"Present"<<key<<" at"<<mid+1;
    break;
    }
    else
        right=mid-1;
        mid=(left+right)/2;
    }
    if(left>right)
        cout<<"Not Present"<<key<<endl;
    return 0;
}


OUTPUT:

5
12 23 36 39 41
39
Present39 at4
Process returned 0 (0x0)   execution time : 26.645 s
Press any key to continue.
